<html>
<head>
    <title>Message Board</title>
    <link rel="stylesheet" href="/styles.css">
</head>
<body>
    <h1><%= title %></h1>
    <a href="/new">New Message</a>
    <ul>
        <% messages.forEach((message) => { %>
            <li>
                <p class="user"><%= message.user %></p>
                <p class="text"><%= message.text %></p>
                <p class="added"><%= message.added %></p>
            </li>
        <% }); %>
    </ul>
</body>
</html>